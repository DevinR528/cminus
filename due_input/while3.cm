linked fn write<T>(_: T);

fn main () {
    let a = 0;
    while (a < 50) {
        let b = 0;
        while (b < 10) {
            write::<int> (a+b);
            b = b + 2;
        }
        let c = 0 - 10;
        while (c != b) {
            write::<int>(c);
            c = c + 1;
        }
        a = a + 10;
    }
}
