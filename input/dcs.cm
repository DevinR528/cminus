const x: int = 0;
const y: int = 0;
const temp: int = 0;

fn fact(a: int): int {
    if (a == 1) {
        return 1;
    } else {
        return fact(a - 1) * a;
    }
}

fn main() {
    read(x);
    read(y);
    temp = y + fact(5) - fact(x);
    write(temp);
    write(x);
    write(y);
}
