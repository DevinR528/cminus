linked fn printf<T>(_fmt: cstr, _: T);

fn add(a: int): int {
    return a + 1;
}

fn main () {
    let i=1; let j=2; let k=3; let l=4;

    let m = i + 7;
    printf("%d\n", m);
    let s0 = l - 5;
    printf("%d\n", s0);
    printf("%d\n", j*k*l);
    printf("%d\n", j*k-l);
    printf("five %d\n", i+j+k*l);
    printf("%d\n", (k*l)/j-i);
    printf("%d\n", (i+j)*(k + (i*m))/s0);
    printf("%d\n", add(10));
}
