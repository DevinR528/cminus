/*
Test function calls as parameters

Eric Johnson    ejohn
*/
linked fn write(_: int);
linked fn read(_: int);


const i: int = 0;

fn print (x: int): int
{
    write (x);
    return 0;
}

fn incre (x: int): int
{
    return x + 1;
}

fn sqr (x: int): int
{
    return x * x;
}

fn main()
{
    read (i);
    while (i)
    {
        print (i);
        print (incre (sqr (incre (i))));
        read (i);
    }
}
