linked fn write(_: int);
linked fn read(_: int);

const X: int = 0;
const Y: int = 0;

fn gcd(x: int, y: int): int {
    let t = 0;
    if (y == 0) {
        return x;
    } else {
        t = x;
        x = y;
        y = t - y * (t / y);
        return gcd(x,y);
    }
}

fn main() {
    read(X);
    read(Y);
    while ((X != 0) || (Y != 0)) {
    write(gcd(X,Y));
        read(X);
        read(Y);
    }
    exit;
}
