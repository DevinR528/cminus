enum option<T> {
    some(T),
    none,
};

enum thing {
    a, b, c, d, e, f, g
};

void main() {
    enum thing x;
    x = thing::a;
    enum option<thing> opt;
    opt = option::some(x);
    match opt {
        option::some(a) -> {
            write(a);
        },
        option::none -> {
            exit;
        }
    }
}
