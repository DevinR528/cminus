enum option<T> {
    some(T),
    none,
};

enum thing {
    a, b, c, d, e, f, g
};

enum option<T> foo<T>(T it) {
    enum option<T> opt;
    opt = option::some(it);
}

void main() {
    enum thing x;
    x = thing::a;
    
    enum option<thing> opt;
    opt = option::some(x);

    enum option<int> num;
    num = foo[int](10);

