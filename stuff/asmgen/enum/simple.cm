linked fn write<T>(_: T);

enum option {
    foo(char),
    some,
    none,
}

fn main() {
    let x = option::foo('c');
    match x {
        // TODO: if this is `x` weird stuff happens...
        option::foo(ch) -> {
            write::<char>(ch);
        },
        option::some -> {
            write::<char>('x');
        },
        option::none -> {
            exit;
        }
    }
}
