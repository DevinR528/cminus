fn write(_: char) {}

enum option {
    foo(char),
    some,
    none,
}

fn main() {
    let x = enum option::some;

    match x {
        option::foo(x) -> {
            write(x);
        },
        option::some -> {
            write('1');
        },
        option::none -> {
            exit;
        }
    }
}
