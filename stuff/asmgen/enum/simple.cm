fn write(_: char) {}

enum option {
    foo(char),
    some,
    none,
}

fn main() {
    let x = enum option::foo('c');
    match x {
        // TODO: if this is `x` weird stuff happens...
        option::foo(ch) -> {
            write(ch);
        },
        option::some -> {
            write('1');
        },
        option::none -> {
            exit;
        }
    }
}
