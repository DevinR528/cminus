linked fn write<T>(_: T);

trait display<T> {
	fn display(item: T);
}

impl display<int> {
	fn display(item: int) {
		write(item);
		// print int
	}
}

impl display<bool> {
	fn display(item: bool) {
		let x: int = item; // error: type mismatch, since it is known here
		write(item);
		// print bool
	}
}

fn show<T: display>(item: T) {
	let x: int = item; // error: type mismatch
	<<T>::display>(item);
}

fn main() {
	int x, y;
	x = 0; y = 1;
	show::<int>(y);
	show::<bool>(x > y);
}
