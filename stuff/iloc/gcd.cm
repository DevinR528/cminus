linked fn print<T>(_: T);
linked fn scan<T>(_: *T);

fn gcd(a: int, b: int): int {
    if (b == 0) {
        return a;
    } else {
        return gcd(b, a % b);
    }
}

fn main() {
    let x = 0; let y = 0;
    scan(&x); scan(&y);
    while (x != 0 || y != 0) {
        print(gcd(x, y));
        scan(&x); scan(&y);
    }
}
