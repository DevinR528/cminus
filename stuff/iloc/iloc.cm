linked fn print<T>(_: T);

fn main(): void {
    // maths
    let i = 1;
    let y = 3 * i;
    let x = i + 2 - y;
    print((x + 2) << 1);
    let z = 0;
    z += 1;
    z -= 2;
    print(z);

    if (z < 0) {
        print("hello");
    }

    z -= 2;
    while (z < 0) {
        print(z)

        z += 1;
    }
    return;
}
